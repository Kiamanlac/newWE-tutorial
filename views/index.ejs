<!DOCTYPE html>
<html>
	<head>
		<title><%= title %></title>
		<script src="https://cdn.tailwindcss.com"></script>
	</head>
	<body class="bg-gray-50">
		<h1 class="text-3xl text-center font-semibold text-slate-600 mt-12"><%= title %></h1>
		<p class="text-lg text-center text-slate-600 mt-4">Welcome to <%= title %></p>
		<% content.forEach((value, i) => { %>
			<% if (value === 0) { %>
				<%- include('./exception') %>
			<% } else { %>
			<%= 1 / value %>
			<br />
			<% } %>
		<% }) %>
	</body>
</html>
